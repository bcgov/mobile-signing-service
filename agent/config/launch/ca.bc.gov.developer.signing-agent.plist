<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>KeepAlive</key>
        <dict>
            <key>SuccessfulExit</key>
            <false/>
        </dict>
        <key>Label</key>
        <string>ca.bc.gov.developer.signing-agent</string>
        <key>EnvironmentVariables</key>
        <dict>
            <key>PATH</key>
            <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:$PATH</string>
        </dict>
        <key>UserName</key>
        <string>xcode</string>
	<key>GroupName</key>
	<string>staff</string>
	<key>InitGroups</key>
	<true/>
        <key>WorkingDirectory</key>
        <string>/Users/xcode/agent</string>
        <key>ProgramArguments</key>
        <array>
            <string>./scripts/start.sh</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
	<key>SessionCreate</key>
	<true/>
        <key>StartInterval</key>
        <integer>3600</integer>
    	<key>StandardErrorPath</key>
    	<string>/Users/xcode/agent/error.log</string>
    	<key>StandardOutPath</key>
    	<string>/Users/xcode/agent/output.log</string>
    </dict>
</plist>
